{
  "name": "flightctl-ui",
  "version": "0.0.2",
  "description": "Front end for flightctl",
  "repository": "https://github.com/flightctl/flightctl-ui.git",
  "homepage": "https://github.com/flightctl/flightctl-ui.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node app.js",
    "start:dev": "concurrently \"node app.js\" \"webpack serve --host 0.0.0.0 --color --progress --config webpack.dev.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "@patternfly/react-code-editor": "^5.1.2",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-styles": "^5.1.2",
    "@patternfly/react-table": "^5.0.0",
    "@storybook/builder-webpack5": "^7.0.27",
    "axios": "^1.4.0",
    "jsrsasign": "^10.9.0",
    "react": "^18",
    "react-dom": "^18",
    "react-oidc-context": "^3.0.0-beta.0",
    "yaml": "^2.3.4"
  }
}

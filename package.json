{
  "name": "flightctl-ui",
  "version": "0.0.2",
  "description": "Front end for flightctl",
  "repository": "https://github.com/flightctl/flightctl-ui.git",
  "homepage": "https://github.com/flightctl/flightctl-ui.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "npm run ts-node ./node_modules/.bin/webpack --mode=production && npm run dr:surge",
    "start": "node app.js",
    "start:dev": "concurrently \"node app.js\" \"npm run ts-node ./node_modules/.bin/webpack serve --mode=development --color --progress\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "npm run ts-node ./node_modules/.bin/webpack --mode=production --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "gen-types": "node ./scripts/openapi-typescript.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "devDependencies": {
    "@patternfly/react-charts": "^7.1.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.3.0",
    "@storybook/addon-actions": "^7.0.27",
    "@storybook/addon-info": "^4.1.18",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/addons": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react-dom": "^18.2.17",
    "@types/victory": "^33.1.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "imagemin": "^8.0.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "openapi-typescript-codegen": "^0.27.0",
    "postcss": "^8.4.25",
    "prettier": "^3.0.0",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@patternfly/react-code-editor": "^5.1.2",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-log-viewer": "^5.0.0",
    "@patternfly/react-styles": "^5.1.2",
    "@patternfly/react-table": "^5.0.0",
    "@storybook/builder-webpack5": "^7.0.27",
    "axios": "^1.4.0",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "formik": "^2.4.5",
    "js-yaml": "^4.1.0",
    "jsrsasign": "^10.9.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "react": "^18",
    "react-dom": "^18.2.0",
    "react-oidc-context": "^3.0.0-beta.0",
    "react-router-dom": "^6.22.0",
    "socket.io-client": "^4.7.3",
    "ts-node": "^10.9.2",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "yaml": "^2.3.4"
  }
}

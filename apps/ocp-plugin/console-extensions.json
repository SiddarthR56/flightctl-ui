      "path": ["/edge/fleets"],
      "component": { "$codeRef": "FleetsPage" }
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "fctl",
      "perspective": "admin",
      "name": "%plugin__flightctl-plugin~Edge Management%"
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
